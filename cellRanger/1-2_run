wget https://cf.10xgenomics.com/supp/cell-exp/refdata-gex-GRCh38-2020-A.tar.gz
wget https://cf.10xgenomics.com/supp/cell-exp/refdata-gex-mm10-2020-A.tar.gz
wget https://cf.10xgenomics.com/supp/cell-exp/refdata-gex-GRCh38-and-mm10-2020-A.tar.gz

cellranger count --id=rcc_10x \
   --transcriptome=/home/songnsnow/download/refdata-gex-GRCh38-2020-A \
   --fastqs=/home/songnsnow/RCC/HWS/H372TDSX7 \
   --sample=3p_GEX \
   --nosecondary \
   --localmem=200 \
   --localcores=8


; 위의 명령어를 통해 cell ranger를 실행할 수 있습니다.
; --id 는 생성되는 결과의 폴더명이며,
; --fastqs는 fastq 파일이 있는 폴더의 위치,
; --sample은 metadata에 들어가는 샘플 정보,
; --transcriptome는 참조 전사체의 위치 입니다.

export PATH=/home/songnsnow/download/cellranger-7.1.0:$PATH