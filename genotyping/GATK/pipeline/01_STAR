
# 240307

# env: STAR

# runSTAR ------------------------------------------------------------------------------------
STAR --runThreadN 20 \
--genomeDir /data/project/RCC_PBMC_HWS/workflow/bulk/vcf_extraction/references/indices \
--readFilesIn /data/project/mRCC/bulkRNA-seq/CKS_1_1.fastq.gz /data/project/mRCC/bulkRNA-seq/CKS_1_2.fastq.gz \
--readFilesCommand zcat \
--outFileNamePrefix /data/project/mRCC/analysis/bulk_vcf/outs/CKS/01 \
--outSAMtype BAM SortedByCoordinate \
--outBAMsortingThreadN 6 \
--twopassMode Basic \
--sjdbOverhang 100

STAR --runThreadN 20 \
--genomeDir /data/project/RCC_PBMC_HWS/workflow/bulk/vcf_extraction/references/indices \
