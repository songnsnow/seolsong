# in env: picard

picard MarkDuplicates \
    I=/data/project/mRCC/analysis/bulk_vcf/outs/CKS/03MergeBam.bam \
    O=/data/project/mRCC/analysis/bulk_vcf/outs/CKS/04MarkDup.bam \
    CREATE_INDEX=true \
    VALIDATION_STRINGENCY=SILENT \
    METRICS_FILE=/data/project/mRCC/analysis/bulk_vcf/outs/CKS/04Mark.metrics

