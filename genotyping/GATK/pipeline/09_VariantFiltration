gatk VariantFiltration \
    -R /data/project/RCC_PBMC_HWS/workflow/bulk/vcf_extraction/references/Homo_sapiens_assembly38.fasta \
    -V /data/project/mRCC/analysis/bulk_vcf/outs/CKS/08HapCall.g.vcf.gz \
    -O /data/project/mRCC/analysis/bulk_vcf/outs/CKS/09VarFilt.g.vcf.gz \
    --window 35 \
    --cluster 3 \
    --filter-name "FS" \
    --filter "FS > 30.0" \
    --filter-name "QD" \
    --filter "QD < 2.0"

